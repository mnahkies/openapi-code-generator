{
  "name": "openapi-code-generator",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist ",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.json",
    "test": "mocha --require source-map-support/register ./dist/**/*.spec.js",
    "lint": "eslint ./src --cache --fix --ext .js,.jsx,.ts,.tsx",
    "ci-build": "npm run build",
    "ci-test": "nyc mocha --reporter mocha-teamcity-reporter -r ts-node/register './src/**/*.spec.ts'",
    "ci-lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "ci-pipeline": " npm run ci-build && npm run ci-test && npm run ci-lint"
  },
  "author": "Michael Nahkies",
  "license": "MIT",
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/prettier": "^2.7.2",
    "nodemon": "2.0.20",
    "nyc": "*",
    "ts-node": "10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^2.1.1",
    "convict": "^6.2.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "lcov",
      "teamcity",
      "text"
    ],
    "all": true
  }
}
