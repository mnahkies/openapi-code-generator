      "default": "./dist/index.js"
    },
    "./cli": "./dist/cli.js",
    "./web": {
      "types": "./dist/web.d.ts",
      "import": "./dist/web.js",
      "require": "./dist/web.js",
      "default": "./dist/web.js"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm tsconfig.tsbuildinfo",
    "build": "tsc -p ./tsconfig.json",
    "test": "jest"
  },
